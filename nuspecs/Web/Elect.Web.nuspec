<?xml version="1.0"?>
<package>
  <metadata>
    <id>Elect.Web</id>
    <title>Elect AspNetCore Mvc Utilities</title>
    <description>AspNetCore Mvc utilities.</description>
    <summary>AspNetCore Mvc utilities.</summary>
    <authors>Top Nguyen</authors>
    <owners>topnguyen</owners>
    <copyright>https://topnguyen.com</copyright>
    <projectUrl>https://github.com/topnguyen/Elect/tree/master/src/Web/Elect.Web</projectUrl>
    <repository type="git" url="https://github.com/topnguyen/Elect.git"/>
    <license type="expression">MIT</license>
    <icon>Logo.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>aspnetcore elect topnguyen</tags>
    <version>7.0.1</version>

    <dependencies>
      <group targetFramework="netstandard2.1">

        <!-- Elect -->
        <dependency id="Elect.Core" version="7.0.1"/>

        <!-- 3rd Libraries -->
        <dependency id="Microsoft.AspNetCore.Mvc.Core" version="2.2.5" />
        <dependency id="Microsoft.AspNetCore.Mvc.Abstractions" version="2.2.0" />
        <dependency id="Microsoft.AspNetCore.Mvc.ViewFeatures" version="2.2.0" />
        <dependency id="Microsoft.AspNetCore.Routing.Abstractions" version="2.2.0" />
        <dependency id="Microsoft.AspNetCore.Localization" version="2.2.0" />
        <dependency id="Microsoft.AspNetCore.WebUtilities" version="2.2.0" />
        <dependency id="Microsoft.Extensions.FileProviders.Physical" version="7.0.0" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="Logo.png" target=""/>
    <file src="..\..\src\Web\Elect.Web\bin\Elect.Web.dll" target="lib\netstandard2.1"/>
    <file src="..\..\src\Web\Elect.Web\bin\Elect.Web.pdb" target="lib\netstandard2.1"/>
    <file src="..\..\src\Web\Elect.Web\bin\Elect.Web.xml" target="lib\netstandard2.1"/>
    <file src="..\..\src\Web\Elect.Web\**\*.cs" target="src" exclude="**\obj*\**\*.cs"/>
  </files>
</package>