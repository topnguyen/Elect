        <dependency id="System.Drawing.Common" version="9.0.7" />
        <dependency id="SixLabors.ImageSharp" version="3.1.10" />
        <dependency id="SixLabors.ImageSharp.Drawing" version="2.1.6" />
      </group>
    </dependencies>
